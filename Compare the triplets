#include<stdio.h>
int main()
{
    int a[3],b[3],m=0,i,t=0;
    for(i=0;i<3;i++)
    {
        scanf("%d",&a[i]);
    }
    for(i=0;i<3;i++)
    {
        scanf("%d",&b[i]);
    }
    for(i=0;i<3;i++)
    {
        if (a[i]>b[i])
        m++;
        else if(a[i]<b[i])
        t++;
    }
    printf("%d %d",m,t);
}
